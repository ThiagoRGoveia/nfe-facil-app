{
  "name": "app",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build:uat": "vite build --mode uat",
    "build:prod": "vite build --mode production",
    "deploy:uat": "pnpm build:uat && wrangler pages deploy ./dist --branch=uat",
    "deploy:prod": "pnpm build:prod && wrangler pages deploy ./dist --branch=main",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen -w",
    "dev": "vite",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@auth0/auth0-vue": "^2.4.0",
    "@mdi/font": "7.4.47",
    "@tanstack/vue-table": "^8.21.2",
    "@types/js-cookie": "^3.0.6",
    "@vee-validate/zod": "^4.15.0",
    "@vue/apollo-composable": "^4.2.1",
    "@vueuse/core": "^12.7.0",
    "apollo-upload-client": "^18.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "core-js": "^3.37.1",
    "graphql": "^16.10.0",
    "js-cookie": "^3.0.5",
    "lucide-vue-next": "^0.476.0",
    "reka-ui": "^2.0.0",
    "roboto-fontface": "*",
    "sst": "3.11.10",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250525.0",
    "@eslint/js": "^9.14.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.3",
    "@graphql-codegen/typescript-operations": "^4.4.1",
    "@graphql-codegen/typescript-vue-apollo": "^4.1.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/aws-lambda": "8.10.148",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "pinia": "^2.1.7",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.3",
    "unplugin-auto-import": "^19.1.0",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.4.10",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.2.8",
